                ...mockProps,
                sessionTimer: -10
            };
            (0, react_1.render)((0, jsx_runtime_1.jsx)(WorkoutSession_1.WorkoutSessionComponent, { ...invalidProps }));
            // Le timer ne devrait pas afficher de valeurs n√©gatives
            expect(react_1.screen.queryByText('-')).not.toBeInTheDocument();
        });
        it('g√®re des exercices avec des donn√©es manquantes', () => {
            const incompleteSession = {
                ...mockSession,
                exercises: [
                    {
                        id: 'ex-incomplete',
